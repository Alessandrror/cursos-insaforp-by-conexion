Algoritmo CincoOperaciones
	Escribir "Ingrese radio del circulo";
	Leer radioDeCirculo;
	Escribir "Ingrese lado de un cuadrado";
	Leer ladoDeCuadrado;
	areaDeCirculo <- radioDeCirculo * radioDeCirculo * PI;
	areaDeCuadrado <- (ladoDeCuadrado * ladoDeCuadrado);
	Escribir "El area del circulo es: ", areaDeCirculo;
	Escribir "El area del cuadrado es ", areaDeCuadrado;
FinAlgoritmo
